{
    "name": "quick-open-file",
    "displayName": "Quick Open File",
    "description": "This is a extension for quickly open file, that's name start with current word.",
    "version": "0.0.1",
    "publisher": "AlanWalk",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.quickOpenFile"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.quickOpenFile",
            "title": "Quick Open File"
        }],
        "keybindings":[{
                "command":"extension.quickOpenFile",
                "key":"alt+g"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}
